{
  "name": "timer",
  "version": "0.1.0",
  "private": true,
  "main": "src/electron/main.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "npm test -- --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron:dev": "set ELECTRON_START_URL=http://localhost:3000 && electron ."
  },
  "dependencies": {
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-scripts": "1.1.4"
  },
  "devDependencies": {
    "coveralls": "3.0.2",
    "electron": "^1.8.4",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1"
  }
}
